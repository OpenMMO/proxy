<?xml version="1.0" encoding="UTF-8"?>
<configuration status="OFF">
	<appenders>
		<console name="Console" target="SYSTEM_OUT">
			<patternLayout pattern="%d{HH:mm:ss} [%t] %-5level %logger{36} - %msg%n"/>
		</console>
	</appenders>
	<loggers>
		<root level="debug">
			<appenderRef ref="Console"/>
		</root>
	</loggers>
</configuration>